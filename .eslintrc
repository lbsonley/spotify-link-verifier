{
    "root": true,
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "env": {
        "es2022": true,
        "node": true,
        "browser": true
    },
    "extends": ["plugin:unicorn/recommended", "plugin:prettier/recommended"],
    "plugins": ["unicorn", "prettier"],
    "rules": {
        "unicorn/prefer-module": "off"
    }
}